        "mean": {
            "label": "Mean",
            "type": "int",
            "readOnly": true,
            "calculate": "[$a1, $a2, $a3, $a4, $a5, $a6, $a7, $a8, $a9, $a10].reduce((a, b) => a + b) / 5"
        },
        "median": {
            "label": "Median",
            "type": "int",
            "readOnly": true,
            "utils": "function median(...n){n.sort((n,t)=>n-t);let t=Math.floor(n.length/2);return n.length%2==0?(n[t-1]+n[t])/2:n[t]}",
            "calculate": "median($a1,$a2,$a3,$a4,$a5,$a6,$a7,$a8,$a9,$a10)"
        },
        "standardDeviation": {
            "label": "Standard Deviation",
            "type": "int",
            "readOnly": true,
            "calculate": "Math.sqrt([$a1, $a2, $a3, $a4, $a5].map(x => Math.pow(x - $mean, 2)).reduce((a, b) => a + b) / 5)"
        }
    }
